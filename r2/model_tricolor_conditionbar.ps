#include "common.h"

struct 	v2p
{
 	float2 	tc0:		TEXCOORD0;
 	half3 	tc1:		TEXCOORD1;
  	half4	c0:			COLOR0;
};


uniform	float4 		m_actor_params;


half4 	main	(v2p I)	:COLOR
{
	half4	t_base;
	t_base.r=((m_actor_params.z<0.5)&& (I.tc0.x<m_actor_params.z))?1:0;
	t_base.g=((m_actor_params.z>0.25)&& (I.tc0.x<m_actor_params.z))?1:0;
	t_base.b=0;
	t_base.a=(I.tc0.x<m_actor_params.z)?1:0;
		
	return  half4	(t_base.r,t_base.g,t_base.b,t_base.a);
}

